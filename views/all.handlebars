<div class="container">
  <nav>
    <h4>Here are some recent posts:</h4>
    {{#each allPosts as |Posts|}}
      <h3>{{Posts.post_title}}</h3>
      <p>{{Posts.post_body}}</p> 
      <h5>Posted by: {{ Posts.user.user_name }} </h5>
      <p></p>
    {{/each}}
    <a href="/api/login" type="button">Login Here</a>
  </nav>
</div>

<div class="form-group">
        <label for="password-signup">Password:</label>
        <input class="form-input" type="password" id="password-signup" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">Signup</button>
      </div>